            "name": "Mathmathg",
            "email": "gle.mathieu@gmail.com"
        }
    ],
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        }
    ],
    "require": {
        "johnpbloch/wordpress": "5.2.*",
        "wpackagist-theme/twentyseventeen":"2.2",
        "wpackagist-plugin/akismet":"4.1.2",
        "wpackagist-plugin/contact-form-7":"5.1.3",
        "wpackagist-plugin/advanced-custom-fields":"5.8.2",
        "wpackagist-plugin/advanced-custom-fields-font-awesome":"3.0.2",
        "wpackagist-plugin/hello-dolly":"1.7.2"

    },
    "require-dev":{
        "wpackagist-plugin/what-the-file":"1.5.4",
        "wpackagist-plugin/simply-show-hooks":"1.2.1"
    },
    "extra":{
        "wordpress-install-dir": "wp",
        "installer-paths": {
            "content/plugins/{$name}/": ["type:wordpress-plugin"],
            "content/themes/{$name}/": ["type:wordpress-theme"]
        }

    }
}
